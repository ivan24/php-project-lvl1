            "src/Games/OddOrEven.php"
        ]
    },
    "authors": [
        {
            "name": "ivan areshkau",
            "email": "bozercov@gmail.com"
        }
    ],

    "require": {
        "php": "^8.0",
        "wp-cli/php-cli-tools": "^0.11.13"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    }
}
