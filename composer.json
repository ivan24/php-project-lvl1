        "bin/brain-progression",
        "bin/brain-calc",
        "bin/brain-gcd",
        "bin/brain-even"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Brain\\": "src/"
        },
        "files": [
            "src/Cli.php",
